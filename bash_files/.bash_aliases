alias ?="tldr"
alias addals="vim ~/.bash_aliases"
alias rldals="source ~/.bash_aliases"
alias rldbash="source ~/.bashrc"
alias pfind="ps -aux | grep "
alias pkill="kill -9 "
alias tb="tensorboard --logdir "
alias gpus="watch -n 1 nvidia-smi"
alias pgpu="fuser -v /dev/nvidia*"
alias gs="git status"
alias gaddpy="git add *.py */*.py"
alias glog="git log --all --oneline --decorate"
alias gfiles="git diff-tree --no-commit-id --name-only -r "
alias gcommit="git commit -m "
alias gpush="git push origin "
alias frcrm="sudo rm -rf "
